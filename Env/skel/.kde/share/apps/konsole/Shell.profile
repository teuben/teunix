[Appearance]
AntiAliasFonts=false
ColorScheme=Xterm
Font=Fixed,10,-1,5,75,0,0,0,0,0

[Cursor Options]
CustomCursorColor=48,208,48
UseCustomCursorColor=true

[General]
Command=/bin/bash
Environment=TERM=xterm
#Icon=luna
LocalTabTitleFormat=%w
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%w
SilenceSeconds=15

[Interaction Options]
AutoCopySelectedText=true
TrimTrailingSpacesInSelectedText=true

[Keyboard]
KeyBindings=default

[Scrolling]
HistorySize=2048
ScrollBarPosition=1

[Terminal Features]
BidiRenderingEnabled=false
